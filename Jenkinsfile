pipeline {
  agent any
  stages {
    stage('build'){
      steps {
        echo "build step"
        sleep 9
      }
    }
    stage('test'){
      steps {
        echo "test step"
        sleep 9
      }
    }
    stage('deploy'){
      steps {
        echo "deploy step"
        sleep 9
      }
    }
    stage('docker'){
      steps {
        echo "image step"
        sleep 9
      }
    }
  }
}
